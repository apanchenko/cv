datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider  = "prisma-client-py"
  interface = "asyncio"
  recursive_type_depth = 5
}

model user {
  id        String    @id @default(cuid()) @db.VarChar(255)
  name      String?   @db.VarChar(255)
  created   DateTime? @default(now())
  updated   DateTime? @default(now())
}
